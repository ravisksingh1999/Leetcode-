class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& matrix) {
         vector<vector<int>> ans;
          vector<int> v;
         for(int i=0; i<matrix[0].size(); i++)
         {
             for(int j=0; j<matrix.size(); j++)
             {
                 
                  v.push_back(matrix[j][i]);
            
             }
                 ans.push_back(v);
                 v={};
         }
      return ans;  
    }
};
